    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "clsx": "^2.0.0",
    "feed": "^4.2.2",
    "focus-visible": "^5.2.0",
    "gray-matter": "^4.0.3",
    "image-size": "^1.0.2",
