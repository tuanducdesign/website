{
  "name": "tuanducdesign.com",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "dev": "node lib/jsonGenerator.js && next dev",
    "dev:watch": "node lib/jsonGenerator.js && next-remote-watch ./data",
    "build": "node lib/jsonGenerator.js && next build && next-sitemap",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts",
    "prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "browserslist": [
    ">= 1%"
  ],
  "dependencies": {
    "@badrap/bar-of-progress": "^0.2.2",
    "@headlessui/react": "^1.7.10",
    "@juggle/resize-observer": "^3.4.0",
    "@reach/rect": "^0.18.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@vercel/analytics": "^0.1.8",
    "clsx": "^1.2.1",
    "focus-visible": "^5.2.0",
    "gray-matter": "^4.0.3",
    "image-size": "1.0.2",
    "intersection-observer": "^0.12.2",
    "mini-svg-data-uri": "^1.4.4",
    "next": "^13.1.6",
    "next-mdx-remote": "^4.3.0",
    "next-remote-watch": "^2.0.0",
    "next-seo": "^5.15.0",
    "next-sitemap": "^3.1.52",
    "postcss-focus-visible": "^8.0.2",
    "postcss-import": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-intersection-observer": "^9.4.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.5.1",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "sharp": "^0.29.3",
    "shiki": "^0.14.1",
    "slug": "^8.2.2",
    "turbo": "^1.7.4",
    "typescript": "^4.9.5",
    "unist-util-visit": "^4.1.2",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.1.6",
    "@svgr/webpack": "^6.5.1",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "tailwindcss": "^3.2.6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@next/env"
      ]
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
