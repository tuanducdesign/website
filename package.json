{
  "name": "tuanducdesign.com",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "start": "next",
    "dev": "concurrently \"next dev\" \"yarn dev:tailwind\"",
    "dev:tailwind": "TAILWIND_MODE=watch yarn postcss css/tailwind.css -o css/build.css --watch",
    "build": "next build && next-sitemap",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@badrap/bar-of-progress": "^0.2.2",
    "@headlessui/react": "^1.7.7",
    "@loadable/component": "^5.15.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "classnames": "^2.3.2",
    "depcheck": "^1.4.3",
    "esbuild": "^0.17.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.2",
    "mdx-bundler": "^9.0.0",
    "mini-svg-data-uri": "^1.4.4",
    "moment": "^2.29.4",
    "next": "13.1.2",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.15.0",
    "next-sitemap": "^3.1.44",
    "next-themes": "^0.2.0",
    "postcss": "^8.4.5",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-lightningcss": "^0.6.0",
    "preact": "^10.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-citation": "^0.4.0",
    "rehype-katex": "^6.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.1.3",
    "rehype-slug": "^5.0.0",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "sharp": "^0.28.3",
    "slugify": "^1.6.5",
    "tailwindcss": "^3.0.23",
    "unist-util-visit": "^4.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.1.2",
    "@svgr/webpack": "^6.5.1",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint": "^7.29.0",
    "eslint-config-next": "13.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.0.0",
    "globby": "11.0.3",
    "husky": "^8.0.0",
    "inquirer": "^8.1.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.2.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
